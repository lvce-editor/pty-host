{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "WebWorker"],
    "module": "esnext",
    "types": [],
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "assumeChangesOnlyAffectDirectDependencies": true,
    "strict": true,
    "noImplicitAny": false,
    "composite": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}


